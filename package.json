{
  "name": "geobot",
  "version": "1.1.0",
  "description": "discord bot for geotastic",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "tsc -p tsconfig.json && node -r dotenv/config src/index.js",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "mocha -r ts-node/register \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduke2k/geobot.git"
  },
